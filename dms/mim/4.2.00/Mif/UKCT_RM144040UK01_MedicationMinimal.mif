                maximumMultiplicity="1" minimumMultiplicity="1"
                name="expectedUseTime" sortKey="2">
                <annotations>
                    <otherAnnotation type="html">
                        <historyItem dateTime="2006-07-10T11:55:11" id="{93C2F6E8-A48D-4C5B-84DE-3C27AED4504D}"/>
                        <text>
