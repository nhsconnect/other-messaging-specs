					<text>
						<div class="FCE96DC0-552C-11DB-85E0-EE1AC0A5204E">
							<strong>
								<font face="times new roman,times" size="3">Parent class linking to and containing information about a patient.</font>
							</strong>
						</div>
					</text>
				</otherAnnotation>
			</annotations>
			<attribute conformance="R" fixedValue="PAT" isMandatory="true" maximumMultiplicity="1" minimumMultiplicity="1" name="classCode" sortKey="1" isStructural="true">
				<annotations>
					<otherAnnotation type="html">
						<historyItem dateTime="2006-09-22T15:16:12" id="{8C0EC889-3C1A-E00D-6A88-491E38A37ECA}"/>
						<text>
							<div class="CC1F8A70-552D-11DB-9F8D-929938389B48">
								<font face="times new roman,times" size="3">Indicates that this is a Patient role.</font>
							</div>
						</text>
					</otherAnnotation>
				</annotations>
				<derivationSupplier attributeName="classCode" className="Patient" staticModelDerivationId="1"/>
