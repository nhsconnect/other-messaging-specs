			<attribute name="effectiveTime" sortKey="10" minimumMultiplicity="1" maximumMultiplicity="1" isMandatory="true" conformance="R">
				<annotations>
					<otherAnnotation type="html">
						<historyItem dateTime="2007-01-25T16:10:12" id="{DB3BC689-214D-4322-B904-8DE3BD7BEAB5}"/>
						<text>
							<p>The time period in which this information is
              valid, represented as a range, supplied in the 
              <em>low</em>and 
              <em>high</em>sub-elements. Both the low and high
              sub-elements are optional, allowing open-ended ranges
              to be specified.</p>
						</text>
					</otherAnnotation>
				</annotations>
				<derivationSupplier staticModelDerivationId="1" className="Act" attributeName="effectiveTime"/>
				<type name="IVL">
					<supplierBindingArgumentDatatype name="TS"/>
				</type>
			</attribute>
			<attribute name="priorityCode" sortKey="13" minimumMultiplicity="0" maximumMultiplicity="1" isMandatory="false">
