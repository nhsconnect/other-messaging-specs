					<text>
						<p>
							<strong>
								<font face="times new roman,times" size="3">The organisation acting as health
                      care provider, i.e. performing the patient
                      care provision.</font>
							</strong>
						</p>
					</text>
				</otherAnnotation>
			</annotations>
			<attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1" fixedValue="ASSIGNED" isMandatory="true" conformance="R" isStructural="true">
				<annotations>
