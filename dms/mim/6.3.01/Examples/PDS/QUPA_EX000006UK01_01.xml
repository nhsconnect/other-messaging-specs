<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="urn:hl7-org:v3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:hl7-org:v3 ../../Schemas/QUPA_MT000006UK01.xsd">
	<person.birthTime>
		<semanticsText>Person.birthTime</semanticsText>
		<value value="19850110" />
	</person.birthTime>
	<person.id>
		<!-- New style NHS Number-->
		<semanticsText>Person.id</semanticsText>
		<value extension="1234567" root="2.16.840.1.113883.2.1.4.1" />
	</person.id>
	<person.name>
			<!-- Usual name with initial -->
			<semanticsText>Person.name</semanticsText>
			<value>
				<given>Z</given>
				<family>Editestpatient</family>
			</value>
	</person.name>
</query>
